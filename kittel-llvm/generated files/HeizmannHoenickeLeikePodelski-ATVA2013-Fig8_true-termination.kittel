///*** 1_main ***///
eval_main_start() -> eval_main_entry_in()
eval_main_entry_in() -> eval_main_while.cond_in(nondef.1, nondef.0) [ 2*nondef.1 >= 1 ]
eval_main_entry_in() -> eval_main_if.end_in() [ 2*nondef.1 < 1 ]
eval_main_while.cond_in(v_call1, v_x.0) -> eval_main_while.body_in(v_call1, v_x.0) [ v_x.0 >= 0 ]
eval_main_while.cond_in(v_call1, v_x.0) -> eval_main_if.end_in() [ v_x.0 < 0 ]
eval_main_while.body_in(v_call1, v_x.0) -> eval_main_while.cond_in(v_call1, v_x.0 - 2*v_call1 + 1)
eval_main_if.end_in() -> eval_main_stop()
